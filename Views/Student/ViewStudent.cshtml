@model StudentManagement.Models.StudentViewModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<b>@ViewBag.message</b>
<table class="table">
    
    <tbody>
        <tr>
            <td>Image</td>
            <td><img src="@Model.ImageFilePath" width="100" height="100" /></td>
        </tr>
        <tr>
            <td>Name</td>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <td>Email</td>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <td>Gender</td>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <td>Course</td>
            <td>@Model.Course</td>
        </tr>
        <tr>
            <td>Fees</td>
            <td>@Model.Fees</td>
        </tr>
    </tbody>
</table>
